{
  "opts": {
    "recurse": true,
    "destination": "./docs/apis/",
    "encoding": "utf8"
  },
  "source": {
    "incldue": ["srv"],
    "includePattern": ".+\\.js$",
    "exclude": ["test"],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": ["jsdoc-to-markdown"],
  "templates": {
    "default": {
      "outputSourceFiles": false
    }
  }
}
